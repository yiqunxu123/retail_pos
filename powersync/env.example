# PowerSync Environment Variables
# Copy this file to .env and fill in your values

# Supabase Database Connection
PS_SUPABASE_DB_HOST=db.YOUR_PROJECT_REF.supabase.co
PS_SUPABASE_DB_PORT=5432
PS_SUPABASE_DB_NAME=postgres
PS_SUPABASE_DB_USER=postgres
PS_SUPABASE_DB_PASSWORD=your-database-password

# Supabase JWKS for JWT validation
# Get this from: curl https://YOUR_PROJECT_REF.supabase.co/auth/v1/.well-known/jwks.json
PS_SUPABASE_JWKS={"keys":[...]}
